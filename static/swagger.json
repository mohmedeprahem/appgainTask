{
  "openapi": "3.0.3",
  "info": {
    "title": "Swagger Petstore - OpenAPI 3.0",
    "description": "Test",
    "termsOfService": "http://swagger.io/terms/",
    "contact": {
      "email": "apiteam@swagger.io"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.11"
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  },
  "tags": [
    {
      "name": "item",
      "description": "Operations about item"
    },
    {
      "name": "order",
      "description": "Access to cart orders"
    },
    {
      "name": "user",
      "description": "Operations about user"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "paths": {
    "/items/add": {
      "post": {
        "tags": ["item"],
        "summary": "Create a new item",
        "description": "add new item to the store",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "count": {
                    "type": "integer",
                    "minimum": 1
                  },
                  "price": {
                    "type": "number",
                    "minimum": 0.01
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {}
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          }
        }
      }
    },
    "/items/get": {
      "get": {
        "tags": ["item"],
        "summary": "Get all items",
        "description": "Returns all items",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {}
            }
          },
          "404": {
            "description": "Not found"
          }
        }
      }
    },
    "/auth/register": {
      "post": {
        "tags": ["user"],
        "summary": "Create a new user",
        "description": "Create a new user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string",
                    "default": "m@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "default": "12345678"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {}
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": ["user"],
        "summary": "Login user",
        "description": "Login user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "default": "m@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "default": "12345678"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {}
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          }
        }
      }
    },
    "/orders": {
      "post": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": ["order"],
        "summary": "Create a new order",
        "description": "Create a new order",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "default": "66342f884bbc8b6ce33e53fb"
                    },
                    "count": {
                      "type": "integer",
                      "format": "int32"
                    }
                  },
                  "required": ["id", "count"]
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {}
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          }
        }
      }
    }
  }
}
